<% if !@notice %>
	<% user = User.find(@relationship.followed_id) %>
	$('#unfollow<%= @relationship.followed_id %>').html("<%= j render partial: 'shared/link_to_follow', locals: {user: user} %>");
	$('.notice-msg').html("Successfully unfollowed!");
<% else %>
	$('.notice-msg').html("<%= j @notice %>");
<% end %>	
timeOut();