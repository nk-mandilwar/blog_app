<h1 id="header-id">Sent Request</h1>
<table class="table table-hover">
  <thead>
    <th colspan="10">User id</th>   
  </thead>
  <tbody>
    <% @outgoing.each do |user| %>
      <tr>
        <td colspan="10"><%= user.user_id %></td>
        <td colspan="10"><%= link_to "Cancel request", user, method: :delete, 
                        data: { confirm: 'Are you sure?' } %> </td>
      </tr>
    <% end %>
  </tbody>
</table>