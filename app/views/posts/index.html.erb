<h1 id="header-id">Listing Posts</h1>
<%= paginate @posts %>
<table class="table table-hover">
  <thead>
    <tr>
      <th>Title</th>
    </tr>
  </thead>

  <tbody>
    <% @posts.each do |post| %>
      <tr>
        <td><%= link_to post.title, post %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= paginate @posts %>



